{
  "expo": {
    "name": "myApp",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "output": "single",
      "bundler": "metro"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "android": {
      "package": "com.myapp",
      "googleServicesFile": "./google-services.json"
    },
    "extra": {
      "eas": {
        "projectId": "06124477-e896-46fd-b1f5-40ea1b4278e9"
      }
    }
  }
}